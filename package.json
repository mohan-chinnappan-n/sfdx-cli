{
  "name": "sfdx-cli",
  "description": "SalesforceDX CLI",
  "version": "6.0.3",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "bin": {
    "sfdx": "bin/run",
    "sfdx_sign": "dist/codeSigning/packAndSign.js"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "cli": {
    "channel": "alpha"
  },
  "cli-engine": {
    "bin": "sfdx",
    "dirname": "sfdx",
    "node": "8.6.0",
    "userPlugins": true,
    "commands": "./dist/commands",
    "legacyConverter": "./dist/legacy/converter",
    "hooks": {
      "plugins:preinstall": [
        "./dist/hooks/plugins/verifyInstallVersion.js",
        "./dist/hooks/plugins/verifyInstallSignature.js"
      ],
      "plugins:parse": [
        "./dist/hooks/plugins/convertFromV5.js"
      ],
      "init": [
        "./dist/hooks/plugins/migrate.js"
      ],
      "prerun": [
        "./dist/hooks/logAnalytics.js"
      ],
      "update": []
    },
    "plugins": [
      "salesforcedx"
    ],
    "s3": {
      "bucket": "dfc-data-production",
      "host": "developer.salesforce.com/media/salesforce-cli"
    },
    "help": {
      "plugins": {
        "salesforcedx": {
          "version": "41.2.0",
          "type": "core"
        }
      }
    }
  },
  "dependencies": {
    "ansi-escapes": "3.0.0",
    "cli-engine": "4.6.17",
    "cli-engine-command": "9.0.2",
    "cli-engine-config": "3.4.0",
    "cli-engine-heroku": "5.0.0",
    "cli-ux": "2.0.3",
    "debug": "3.0.1",
    "fs-extra": "4.0.2",
    "lodash": "4.11.1",
    "salesforcedx": "41.2.3-0"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "4.0.2",
    "@types/lodash": "4.14.76",
    "@types/mocha": "2.2.43",
    "@types/node": "8.0.28",
    "@types/request": "2.0.3",
    "@types/sinon": "2.3.5",
    "chai": "4.1.2",
    "mocha": "3.5.0",
    "sinon": "2.3.5",
    "tslint": "5.7.0",
    "typescript": "2.5.2"
  },
  "scripts": {
    "clean": "bash scripts/clean",
    "build": "scripts/build",
    "clean-all": "scripts/clean-all",
    "clean-cache": "scripts/clean-cache",
    "clean-dev": "scripts/clean-dev",
    "downgrade": "scripts/downgrade",
    "kill-updaters": "scripts/kill-updaters",
    "lint": "tslint -p tslint.json",
    "prepare": "tsc -p tsconfig.json",
    "release-all": "scripts/release-all",
    "scorch-the-earth": "scripts/scorch-the-earth",
    "setup": "scripts/setup",
    "test": "yarn typings && yarn prepare && yarn lint && mocha --recursive \"dist/**/*.test.js\"",
    "typings": "tsc -p typings/tsconfig.json",
    "watch": "tsc -w -p tsconfig.json"
  }
}
