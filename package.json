{
  "name": "sfdx-cli",
  "description": "Salesforce CLI",
  "version": "6.36.1-100",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "bin": {
    "sfdx": "bin/run"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    "bin",
    "oclif.manifest.json",
    "dist/**/*.js",
    "!dist/**/*.test.js"
  ],
  "oclif": {
    "bin": "sfdx",
    "commands": "./dist/commands",
    "node": "8.12.0",
    "dirname": "sfdx",
    "hooks": {
      "plugins:preinstall": [
        "./dist/hooks/pluginsPreinstall.js"
      ],
      "plugins:preinstall:verify:version": [
        "./dist/hooks/verifyPluginVersion.js"
      ],
      "preupdate": [
        "./dist/hooks/updateReachability.js"
      ],
      "update": [
        "./dist/hooks/lazyRequire.js"
      ]
    },
    "plugins": [
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-which",
      "@salesforce/plugin-generator",
      "@salesforce/plugin-analytics",
      "@salesforce/sfdx-trust",
      "salesforcedx"
    ],
    "update": {
      "s3": {
        "bucket": "dfc-data-production",
        "folder": "media/salesforce-cli",
        "host": "developer.salesforce.com/media/salesforce-cli",
        "templates": {
          "target": {
            "baseDir": "sfdx-cli",
            "manifest": "<%- 'sfdx-cli/channels/' + channel + '/' %><%- platform %>-<%- arch %>",
            "unversioned": "<%- 'sfdx-cli/channels/' + channel + '/' %><%- bin %>-<%- platform %>-<%- arch %><%- ext %>",
            "versioned": "<%- 'sfdx-cli/channels/' + channel + '/' %><%- bin %>-v<%- version %>/<%- bin %>-v<%- version %>-<%- platform %>-<%- arch %><%- ext %>"
          },
          "vanilla": {
            "baseDir": "sfdx-cli",
            "manifest": "<%- 'sfdx-cli/channels/' + channel + '/' %>version",
            "unversioned": "<%- 'sfdx-cli/channels/' + channel + '/' %><%- bin %><%- ext %>",
            "versioned": "<%- 'sfdx-cli/channels/' + channel + '/' %><%- bin %>-v<%- version %>/<%- bin %>-v<%- version %><%- ext %>"
          }
        }
      }
    }
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-commands": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1",
    "@oclif/plugin-plugins": "^1",
    "@oclif/plugin-update": "^1",
    "@oclif/plugin-warn-if-update-available": "^1",
    "@oclif/plugin-which": "^1",
    "@salesforce/kit": "0.7.0",
    "@salesforce/lazy-require": "0.0.4",
    "@salesforce/plugin-analytics": "0.3.0",
    "@salesforce/plugin-generator": "0.0.10",
    "@salesforce/require-analytics": "0.4.0",
    "@salesforce/sfdx-trust": "2.0.0",
    "@salesforce/ts-types": "0.13.0",
    "ansi-escapes": "^3",
    "chalk": "^2",
    "debug": "^3",
    "fs-extra": "^4",
    "request": "^2",
    "salesforcedx": "43.17.1-101",
    "tslib": "^1",
    "v8-compile-cache": "2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@salesforce/dev-config": "^1",
    "@salesforce/ts-sinon": "0.2.0",
    "@types/chai": "^4",
    "@types/chalk": "2.2.0",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "4.0.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/request": "2.0.3",
    "@types/sinon": "4.3.2",
    "chai": "^4",
    "eslint": "^3",
    "eslint-config-airbnb-base": "5.0.1",
    "eslint-plugin-import": "1.13.0",
    "globby": "^8",
    "husky": "1.1.0",
    "mocha": "^3",
    "nyc": "^13",
    "sinon": "^5",
    "source-map-support": "0.5.2",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "^2.9",
    "xunit-file": "1.0.0"
  },
  "scripts": {
    "build": "yarn build-standalone",
    "build-mac": "yarn build-mac-from-archive",
    "build-mac-from-archive": "scripts/build/installers/darwin_from_archive",
    "build-mac-from-sources": "scripts/build/installers/darwin_from_sources",
    "build-standalone": "scripts/build/standalone",
    "build-windows": "scripts/build/installers/windows",
    "clean": "yarn clean-dist && yarn clean-test",
    "clean-all": "scripts/clean/all",
    "clean-build": "scripts/clean/build",
    "clean-compile": "yarn clean-all && yarn install --pure-lockfile",
    "clean-dist": "scripts/clean/dist",
    "clean-test": "scripts/clean/test",
    "compile": "scripts/build/compile",
    "coverage-report": "scripts/test/coverage",
    "lint": "scripts/lint/sources",
    "lint-all": "scripts/lint/all",
    "lint-with-style": "scripts/lint/sources_with_style",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "prepack": "yarn prepare",
    "prepare": "yarn clean && yarn compile && yarn lint && oclif-dev manifest",
    "release": "yarn release-all",
    "release-debug": "yarn release-all-debug",
    "release-all": "scripts/release/all",
    "release-all-debug": "env SKIP_AWS=true yarn release-all",
    "release-mac": "scripts/release/installers/darwin",
    "release-mac-debug": "env SKIP_AWS=true yarn release-mac",
    "release-standalone": "scripts/release/standalone",
    "release-standalone-debug": "env SKIP_AWS=true yarn release-standalone",
    "release-windows": "scripts/release/installers/windows_docker",
    "release-windows-debug": "env SKIP_AWS=true yarn release-windows",
    "test": "scripts/test/all",
    "typings": "scripts/test/typings",
    "unit": "scripts/test/unit",
    "uts": "nyc mocha --require source-map-support/register --recursive \"dist/**/*.test.js\"",
    "watch": "tsc -w -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn compile && yarn lint && yarn uts"
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 75,
    "branches": 75,
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "dist/**/*.test.js"
    ]
  }
}
