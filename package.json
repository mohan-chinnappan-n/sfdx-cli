{
  "name": "sfdx-cli",
  "description": "SalesforceDX CLI",
  "version": "6.0.0",
  "author": "Salesforce",
  "bin": {
    "sfdx": "bin/run"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "cli": {
    "channel": "alpha"
  },
  "cli-engine": {
    "bin": "sfdx",
    "dirname": "sfdx",
    "node": "8.4.0",
    "userPlugins": true,
    "hooks": {
      "plugins:preinstall": "./lib/hooks/preinstall.js",
      "init": "./lib/hooks/init.js",
      "prerun": "./lib/hooks/prerun.js"
    },
    "plugins": [
      "salesforcedx"
    ],
    "s3": {
      "bucket": "dfc-data-production",
      "host": "developer.salesforce.com/media/salesforce-cli"
    }
  },
  "dependencies": {
    "cli-engine": "4.3.1",
    "salesforcedx": "41.2.0-v6.1"
  },
  "scripts": {
    "build": "scripts/build",
    "clean-all": "scripts/clean-all",
    "clean-cache": "scripts/clean-cache",
    "clean-dev": "scripts/clean-dev",
    "downgrade": "scripts/downgrade",
    "release-all": "scripts/release-all",
    "scorch-the-earth": "scripts/scorch-the-earth",
    "setup": "scripts/setup",
    "test": "mocha --recursive lib"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "chai": "4.1.2",
    "mocha": "3.5.0"
  }
}
