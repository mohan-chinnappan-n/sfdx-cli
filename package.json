{
  "name": "sfdx-cli",
  "description": "SalesforceDX CLI",
  "version": "6.0.0",
  "author": "Salesforce",
  "bin": {
    "sfdx": "bin/run",
    "sfdx_sign": "dist/codeSigning/packAndSign.js"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "cli": {
    "channel": "alpha"
  },
  "cli-engine": {
    "bin": "sfdx",
    "dirname": "sfdx",
    "node": "8.4.0",
    "userPlugins": true,
    "hooks": {
      "plugins:preinstall": "./dist/hooks/preinstall.js",
      "init": "./dist/hooks/init.js",
      "prerun": "./dist/hooks/prerun.js"
    },
    "plugins": [
      "salesforcedx"
    ],
    "s3": {
      "bucket": "dfc-data-production",
      "host": "developer.salesforce.com/media/salesforce-cli"
    }
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "cli-engine": "4.3.1",
    "fs-extra": "0.30.0",
    "lodash": "4.11.1",
    "salesforcedx": "41.2.0-v6.3",
    "heroku-cli-util": "6.0.16"
  },
  "scripts": {
    "clean": "bash scripts/clean",
    "build": "scripts/build",
    "clean-all": "scripts/clean-all",
    "clean-cache": "scripts/clean-cache",
    "clean-dev": "scripts/clean-dev",
    "downgrade": "scripts/downgrade",
    "lint": "tslint -p tslint.json",
    "prepare": "tsc -p tsconfig.json",
    "release-all": "scripts/release-all",
    "scorch-the-earth": "scripts/scorch-the-earth",
    "setup": "scripts/setup",
    "test": "yarn prepare && yarn lint && mocha --recursive dist",
    "watch": "tsc -w -p tsconfig.json"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/mocha": "2.2.43",
    "@types/node": "8.0.28",
    "chai": "4.1.2",
    "mocha": "3.5.0",
    "tslint": "5.7.0",
    "sinon": "1.17.4",
    "typescript": "2.5.2"
  }
}
